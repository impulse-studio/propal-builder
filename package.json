    "@tailwindcss/postcss": "^4.1.17",
    "@types/node": "^20",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "drizzle-kit": "^0.31.7",
    "tailwindcss": "^4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}

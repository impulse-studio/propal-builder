{
  "name": "propal-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --raw --kill-others pnpm:dev:*",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome format --write",
    "typecheck": "tsc --noEmit",
    "checks": "pnpm run lint && pnpm run typecheck",
    "docker": "./docker/start.sh",
    "dev:next": "next dev --turbopack",
    "dev:db": "pnpm db:studio",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate && pnpm exec biome check --write migrations",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:start": "docker compose -f docker-compose.yml up -d",
    "db:stop": "docker compose -f docker-compose.yml down -v",
    "generate:auth": "pnpx @better-auth/cli generate --output ./db/schema/auth/schema.ts && pnpm exec biome check --write ./db/schema/auth/schema.ts",
    "trigger:deploy": "pnpx trigger.dev@latest deploy",
    "env:pull": "vercel env pull .env",
    "sync": "git fetch template && git merge template/main",
    "sync:initial": "git fetch template && git merge template/main --allow-unrelated-histories",
    "setup:sync": "git remote get-url template >/dev/null 2>&1 && git remote set-url template https://github.com/impulse-studio/minimal-boilerplate.git || git remote add template https://github.com/impulse-studio/minimal-boilerplate.git"
  },
  "dependencies": {
    "@orpc/client": "^1.11.2",
    "@orpc/server": "^1.11.2",
    "@orpc/tanstack-query": "^1.11.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@remixicon/react": "^4.7.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-table": "^8.21.3",
    "@tiptap/extension-highlight": "^3.10.7",
    "@tiptap/extension-link": "^3.10.7",
    "@tiptap/extension-placeholder": "^3.10.7",
    "@tiptap/extension-typography": "^3.10.7",
    "@tiptap/extension-underline": "^3.10.7",
    "@tiptap/react": "^3.10.7",
    "@tiptap/starter-kit": "^3.10.7",
    "@vercel/blob": "^2.0.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.7",
    "merge-refs": "^2.0.0",
    "motion": "^12.23.24",
    "next": "16.0.3",
    "next-themes": "^0.4.6",
    "nuqs": "^2.7.3",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-aria-components": "^1.13.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "19.2.0",
    "react-easy-crop": "^5.5.3",
    "react-hook-form": "^7.66.0",

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5555/db

# Environment
NODE_ENV=development

# Eleven Labs API
# Get your API key from https://elevenlabs.io/
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Attio API (Optional)
# Used for CRM integration
ATTIO_API_KEY=your_attio_api_key_here

# Qdrant Vector Database (Optional)
# Used for vector search and similarity matching
# For local development, you can use Qdrant Cloud or run locally
QDRANT_URL=https://your-cluster.qdrant.io
QDRANT_API_KEY=your_qdrant_api_key_here

# Vercel Environment Variables (Optional - Auto-populated on Vercel)
# These are automatically set when deploying to Vercel
VERCEL_URL=
VERCEL_PROJECT_PRODUCTION_URL=
VERCEL_ENV=development
VERCEL_BRANCH_URL=

# Vercel Blob Storage (Required for file uploads in production)
# Get your token from https://vercel.com/dashboard/stores
# Note: This is automatically set when deployed on Vercel
# For local development, you may need to set this manually
BLOB_READ_WRITE_TOKEN=your_vercel_blob_token_here

